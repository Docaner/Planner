<UserControl x:Class="Planner.Module.Diagram.Components.Agregotorses"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Planner.Module.Diagram.Components"
             xmlns:models="clr-namespace:Planner.Module.Diagram.Models" d:DataContext="{d:DesignInstance Type=models:Agregator}"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="200">
    <Grid>
        <TextBlock x:Name="Name" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding Path=Name}"></TextBlock>
    </Grid>
</UserControl>
